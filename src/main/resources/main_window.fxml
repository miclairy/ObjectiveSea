            <ButtonBar fx:id="btnBar" prefWidth="168.0">
              <buttons>
                <Button fx:id="btnTrack" alignment="CENTER" mnemonicParsing="false" text="Track" visible="false" />
              </buttons>
            </ButtonBar>
                <ImageView fx:id="mapImageView" fitHeight="88.0" fitWidth="100.0" pickOnBounds="true" />
                <VBox fx:id="startersOverlay" alignment="CENTER" blendMode="SRC_ATOP" prefHeight="600.0" prefWidth="400.0" styleClass="startersOverlay" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="100.0">
                    <children>
                        <Label fx:id="startersOverlayTitle" alignment="CENTER" maxWidth="400.0" minWidth="100.0" prefWidth="300.0" text="America's Cup Race">
                            <font>
                                <Font size="23.0" />
                            </font>
                            <padding>
                                <Insets bottom="10.0" top="10.0" />
                            </padding>
                        </Label>
                        <Label fx:id="clockLabel" alignment="CENTER" contentDisplay="CENTER" maxWidth="400.0" minWidth="100.0" prefWidth="300.0" textAlignment="CENTER" textFill="WHITE">
                            <font>
                                <Font size="22.0" />
                            </font>
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                            <padding>
                                <Insets bottom="10.0" />
                            </padding>
                        </Label>
                        <ListView fx:id="startersList" maxHeight="500.0" maxWidth="400.0" minHeight="150.0" minWidth="100.0" mouseTransparent="true" prefHeight="300.0" prefWidth="300.0" />
                    </children>
                </VBox>
                <Group fx:id="root" layoutY="-1.0">
                    <children>
                        <ImageView fx:id="windDirectionImage" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true" x="410.0" y="15.0">
                            <image>
                                <Image url="@graphics/arrow.png" />
                            </image>
                        </ImageView>
                        <Canvas fx:id="canvas" height="400.0" width="298.0" />
                        <Label fx:id="fpsLabel" alignment="CENTER" centerShape="false" contentDisplay="BOTTOM" layoutX="4.0" layoutY="4.0" prefHeight="36.0" prefWidth="64.0" textAlignment="CENTER" />
