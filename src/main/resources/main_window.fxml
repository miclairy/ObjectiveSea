            <ImageView fx:id="mapImageView1" fitHeight="88.0" fitWidth="100.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" />
            <Button layoutX="14.0" layoutY="76.0" mnemonicParsing="false" onAction="#mapZoomed" text="+" />
